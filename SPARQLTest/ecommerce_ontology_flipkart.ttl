@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Flipkart is a subclass of Ecommerce
:Flipkart a rdfs:Class ;
    rdfs:subClassOf :Ecommerce .


# Relations between Flipkart and its Classes
ItemNS:Group a rdfs:Class .
ItemNS:Producer a rdfs:Class .
FlipkartNS:Item a rdfs:Class .
FlipkartNS:Consumer a rdfs:Class .

## Object Properties (relationships from Flipkart to other classes)
FlipkartNS:hasGroup a owl:ObjectProperty ;
    rdfs:domain :Flipkart ;
    rdfs:range ItemNS:Group .

FlipkartNS:hasProducer a owl:ObjectProperty ;
    rdfs:domain :Flipkart ;
    rdfs:range ItemNS:Producer .

FlipkartNS:hasItem a owl:ObjectProperty ;
    rdfs:domain :Flipkart ;
    rdfs:range FlipkartNS:Item .

FlipkartNS:hasConsumer a owl:ObjectProperty ;
    rdfs:domain :Flipkart ;
    rdfs:range FlipkartNS:Consumer .


# Relationships between Item, Producer, and Group

ItemNS:hasProducer a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Producer .

ItemNS:producesItem a owl:ObjectProperty ;
    rdfs:domain ItemNS:Producer ;
    rdfs:range FlipkartNS:Item .

ItemNS:hasGroup a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Group .

ItemNS:hasItem a owl:ObjectProperty ;
    rdfs:domain ItemNS:Group ;
    rdfs:range FlipkartNS:Item .

# Class Sub-Group, relation between Group, Sub-Group and Item
ItemNS:SubGroup a rdfs:Class .

## Relationships
ItemNS:hasSubGroup a owl:ObjectProperty ;
    rdfs:domain ItemNS:Group ;
    rdfs:range ItemNS:SubGroup .

ItemNS:hasItem a owl:ObjectProperty ;
    rdfs:domain ItemNS:SubGroup ;
    rdfs:range FlipkartNS:Item .

# Classes of Item
ItemNS:salePrice a rdfs:Class .
ItemNS:stockStatus a rdfs:Class .
ItemNS:Return_Rules a rdfs:Class .
ItemNS:Rating a rdfs:Class .
ItemNS:Dimensions a rdfs:Class .
ItemNS:Shipping_Methods a rdfs:Class .

# Relationships from Item to above classes
ItemNS:hassalePrice a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:salePrice .

ItemNS:hasstockStatus a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:stockStatus .

ItemNS:hasReturnPolicy a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Return_Rules .

ItemNS:hasRating a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Rating .

ItemNS:hasDimensions a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Dimensions .

ItemNS:hasShippingOptions a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range ItemNS:Shipping_Methods .

ItemNS:Return_Fee a rdfs:Class .

ItemNS:hasReturnFee a owl:ObjectProperty ;
    rdfs:domain ItemNS:Return_Rules ;
    rdfs:range ItemNS:Return_Fee .

# Define Order as a subclass of Item
ItemNS:Order a rdfs:Class ;
    rdfs:subClassOf FlipkartNS:Item .

OrderNS:Shipping_Info a rdfs:Class .

OrderNS:hasShippingInfo a owl:ObjectProperty ;
    rdfs:domain ItemNS:Order ;
    rdfs:range OrderNS:Shipping_Info .

## Shipping_Info of Order
OrderNS:salePrice a rdfs:Class .
OrderNS:Estimated_Delivery_Time a rdfs:Class .
OrderNS:Address a rdfs:Class .

OrderNS:hasShippingsalePrice a owl:ObjectProperty ;
    rdfs:domain OrderNS:Shipping_Info ;
    rdfs:range OrderNS:salePrice .

OrderNS:hasDeliveryTime a owl:ObjectProperty ;
    rdfs:domain OrderNS:Shipping_Info ;
    rdfs:range OrderNS:Estimated_Delivery_Time .

OrderNS:deliverTo a owl:ObjectProperty ;
    rdfs:domain OrderNS:Shipping_Info ; 
    rdfs:range OrderNS:Address .

ItemShippingOptionsNS:salePrice a rdfs:Class .
ItemShippingOptionsNS:Estimated_Delivery_Time a rdfs:Class .

ItemNS:hasShippingsalePrice a owl:ObjectProperty ;
    rdfs:domain ItemNS:Shipping_Methods ;
    rdfs:range ItemShippingOptionsNS:salePrice .


ItemNS:hasDeliveryTime a owl:ObjectProperty ;
    rdfs:domain ItemNS:Shipping_Methods ;
    rdfs:range ItemShippingOptionsNS:Estimated_Delivery_Time .
    

ItemNS:Order a rdfs:Class ;
    rdfs:subClassOf FlipkartNS:Item .
FlipkartNS:Customer a rdfs:Class ;
    rdfs:subClassOf FlipkartNS:Consumer .

FlipkartNS:Seller a rdfs:Class ;
    rdfs:subClassOf FlipkartNS:Consumer .

CustomerNS:views a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Customer ;
    rdfs:range FlipkartNS:Item .

ItemNS:hasSeller a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Item ;
    rdfs:range FlipkartNS:Seller .

SellerNS:sells a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Seller ;
    rdfs:range FlipkartNS:Item .

# Customer namespace classes 
CustomerNS:Cart a rdfs:Class .
CustomerNS:Wishlist a rdfs:Class .
CustomerNS:Details a rdfs:Class .


CustomerNS:hasCart a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Customer ;
    rdfs:range CustomerNS:Cart .

CustomerNS:hasWishlist a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Customer ;
    rdfs:range CustomerNS:Wishlist .

CustomerNS:hasDetails a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Customer ;
    rdfs:range CustomerNS:Details .

CustomerNS:hasOrder a owl:ObjectProperty ;
    rdfs:domain CustomerNS:Cart ;
    rdfs:range ItemNS:Order .

CustomerNS:Address_Options a rdfs:Class .

CustomerNS:hasAddress a owl:ObjectProperty ;
    rdfs:domain CustomerNS:Details ;
    rdfs:domain CustomerNS:Address_Options .

SellerNS:Contact_Info a rdfs:Class .
SellerNS:Performance_Metrics a rdfs:Class .


SellerNS:hasPerformanceMetrics a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Seller ;
    rdfs:range SellerNS:Performance_Metrics .

SellerNS:hasContact a owl:ObjectProperty ;
    rdfs:domain FlipkartNS:Seller ; 
    rdfs:range SellerNS:Contact_Info .


CustomerNS:hasItem a owl:ObjectProperty ;
    rdfs:domain CustomerNS:Wishlist ;
    rdfs:range FlipkartNS:Item .

