@prefix : <http://delta.org#> .
@prefix FlipkartNS: <http://delta.org/flipkart#> .
@prefix CustomerNS: <http://delta.org/flipkart/CustomerNS#> .
@prefix OrderNS: <http://delta.org/flipkart/OrderNS#> .
@prefix ItemNS: <http://delta.org/flipkart/ItemNS#> .
@prefix ItemShippingOptionsNS: <http://delta.org/flipkart/ItemShippingOptionsNS#> .
